{
  "name": "@thrnd/http-proxy",
  "version": "1.2.1",
  "main": "./dist/cjs/proxy-framework.app.js",
  "module": "./dist/esm/proxy-framework.app.js",
  "bin": "./dist/cjs/cli/cli.js",
  "repository": {
    "url": "https://github.com/the-heart-rnd/http-proxy/"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -b tsconfig.build.json && tsc -b tsconfig.build-esm.json",
    "prestart": "check-engines",
    "prepare": "ts-patch install -s",
    "prepublishOnly": "npm run test",
    "test": "jest --coverage",
    "start": "node ./dist/cjs/cli/cli.js",
    "start:dev": "ts-node src/cli/cli.ts",
    "build:watch": "tsc -b --watch",
    "watch": "nodemon --watch dist dist/index.js --",
    "lint": "eslint --fix --ext .ts src"
  },
  "dependencies": {
    "@mutagen-d/node-proxy-server": "^1.0.0",
    "brotli": "^1.3.3",
    "check-engines": "^1.5.0",
    "content-type": "^1.0.4",
    "get-port": "^5.1.1",
    "http-proxy": "^1.18.1",
    "json5": "^2.2.3",
    "pino": "^8.8.0",
    "pino-pretty": "^7.0.0",
    "rambda": "^7.4.0",
    "socks": "^2.7.1",
    "tapable": "^2.2.1",
    "undici": "^5.16.0",
    "url-join": "^4.0.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/brotli": "^1.3.1",
    "@types/content-type": "^1.1.5",
    "@types/http-proxy": "^1.17.9",
    "@types/jest": "^29.2.6",
    "@jest/types": "^29.4.1",
    "@types/node": ">=16",
    "@types/socket.io": "^3.0.2",
    "@types/url-join": "^4.0.1",
    "@types/uuid": "^8.3.4",
    "@types/yargs": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "axios": "^1.2.3",
    "compress-brotli": "^1.3.8",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-preferred-import": "^1.0.12",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-extended": "^3.2.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "rimraf": "^4.1.2",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.1.0",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "engines": {
    "node": ">= 16.0.0"
  }
}
